{
  "name": "summer-camp-applications",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy:dev": "firebase deploy --project development",
    "deploy:prod": "firebase deploy --project production"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "core-js": "^3.4.3",
